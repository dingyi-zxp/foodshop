.c-mf {
	padding: 10px;
	background: #fff;
	border-radius: 5px;
	position:relative;
}

.c-mf-flex{
	display: flex;
	height: 100%;
	width: 100%;
}

.c-mf-flex-divide {

	flex: 1;
}
.c-mf-content {
	width: 100%;
	height: 100%;
}
/* solo module of layout */
.c-ml {
	display: flex;
	align-items: center;
	justify-content: center;
	width: 100%;
	height: 100%;
}

.img{
	width: 96%;
	height: 100%;
}

.img-content {
	position: relative;
	display: flex;
	align-items: center;
	justify-content: center;
	width: 100%;
	height: 90%;

}

/* content text style */
.c-cts {
	position: relative;
	display: flex;
	align-items: center;
	justify-content: center;
	font-weight: 400;
	font-size: 14px;
	color: #B6B6B6;
}

.cut-line {
	top: 13%;
	margin-left: 43%;
	position: absolute;
	height: 150px;
	border-right-style: solid;
	border-width:1px;
	border-color: #B6B6B6

}
