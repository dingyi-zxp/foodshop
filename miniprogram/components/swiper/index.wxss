.c-swiper{
	position: relative;
	width: 100%;
	height: 25vh;
}

.c-swiper-frame{ 
	position: absolute;
	width: 100%;
	height: 30vh;
}

.c-swiper-img{
	width: 100%
}

.c-swiper-dots{
	position: absolute;
	width: 100%;
	display: flex;
	align-items: center;
	top: 22.5vh
	
}

.c-swiper-dots-center{
	
	justify-content: center;
}

.dot_shape{
	height: 1.7vw;
	width: 2vh;
	background: #fff;
}

.dot_edge {
	margin: 0 5px
}

.dot_select{
	background:pink;
}
